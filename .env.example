# Example environment variables for Yatagarasu S3 Proxy
# Copy this file to .env and fill in your values

# AWS/S3 Credentials - Public Bucket
AWS_ACCESS_KEY_PUBLIC=your-access-key-here
AWS_SECRET_KEY_PUBLIC=your-secret-key-here

# AWS/S3 Credentials - Private Bucket
AWS_ACCESS_KEY_PRIVATE=your-access-key-here
AWS_SECRET_KEY_PRIVATE=your-secret-key-here

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-minimum-32-characters-long

# Server Configuration
SERVER_ADDRESS=0.0.0.0:8080
METRICS_PORT=9090

# Logging Configuration
RUST_LOG=info
LOG_FORMAT=json

# For local testing with MinIO (docker-compose)
# AWS_ACCESS_KEY_PUBLIC=minioadmin
# AWS_SECRET_KEY_PUBLIC=minioadmin
# AWS_ACCESS_KEY_PRIVATE=minioadmin
# AWS_SECRET_KEY_PRIVATE=minioadmin
# JWT_SECRET=test-secret-key-for-local-development-only
